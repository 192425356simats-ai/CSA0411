#include <stdio.h>
#include <unistd.h>

int main() {
    pid_t pid = fork();
    
    if (pid < 0) {
        perror("Fork failed");
        return 1;
    } else if (pid == 0) {
        // Child process
        printf("Child process:\n");
        printf("PID: %d, PPID: %d\n", getpid(), getppid());
    } else {
        // Parent process
        printf("Parent process:\n");
        printf("PID: %d, Child PID: %d\n", getpid(), pid);
    }
    return 0;
}
