// multithread.c
#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>

void* threadFunc(void* arg) {
    int* num = (int*)arg;
    printf("Thread %d running\n", *num);
    return NULL;
}

int main() {
    pthread_t tid1, tid2;
    int a = 1, b = 2;

    pthread_create(&tid1, NULL, threadFunc, &a);
    pthread_create(&tid2, NULL, threadFunc, &b);

    pthread_join(tid1, NULL);
    pthread_join(tid2, NULL);

    printf("Main thread exiting.\n");
    return 0;
}
