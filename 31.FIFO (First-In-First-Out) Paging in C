#include <stdio.h>
int main() {
    int frames, pages, i, j, k, page_faults = 0;
    printf("Enter number of frames: ");
    scanf("%d", &frames);
     printf("Enter number of pages: ");
    scanf("%d", &pages);
     int page_sequence[pages];
    printf("Enter page reference string: ");
    for (i = 0; i < pages; i++) {
        scanf("%d", &page_sequence[i]);
    }
int memory[frames];
    for (i = 0; i < frames; i++)
        memory[i] = -1;
int index = 0;
    for (i = 0; i < pages; i++) {
        int found = 0;
        for (j = 0; j < frames; j++) {
            if (memory[j] == page_sequence[i]) {
                found = 1;
                break;
            }
        }
if (!found) {
            memory[index] = page_sequence[i];
            index = (index + 1) % frames;
            page_faults++;
        }
// Display current state
        printf("Frame content after accessing page %d: ", page_sequence[i]);
        for (k = 0; k < frames; k++) {
            if (memory[k] != -1)
                printf("%d ", memory[k]);
            else
                printf("- ");
        }
        printf("\n");
    }
 printf("Total Page Faults: %d\n", page_faults);
    return 0;
}
